<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<VBox fx:id="root" alignment="CENTER" spacing="10" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.AddPlayerController">
    <Label text="Numele jucatorului"/>
    <TextField fx:id="playerName" prefWidth="100"/>
    <Button text="Adauga jucatorul" onAction="#handleAddPlayer"/>
    <Button text="Inapoi" fx:id="backButton" onAction="#handleBackButton"/>
    <stylesheets>
        <URL value="@styles/mainstyle.css" />
    </stylesheets>
</VBox>
